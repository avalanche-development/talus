{
    "name": "jacobemerick/talus",
    "description": "Microframework with Swagger routing",
    "license": "MIT",
    "authors": [
        {
            "name": "jacobemerick",
            "email": "jpemeric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "^1.0",
        "thefrozenfire/swagger": "2.0.x-dev#ef27c84532a8d0b98d76fa40381b12b3703d1bfc as 2.0.8",
        "zendframework/zend-diactoros": "^1.3",
        "psr/http-message": "^1.0",
        "container-interop/container-interop": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jacobemerick/PHP-Swagger-Parser"
        }
    ],
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Jacobemerick\\Talus\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/function-overrides.php"
        ],
        "psr-4": {
            "Jacobemerick\\Talus\\": "tests/"
        }
    }
}
