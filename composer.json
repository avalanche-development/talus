{
    "name": "avalanche-development/talus",
    "description": "Microframework with Swagger routing",
    "license": "MIT",
    "authors": [
        {
            "name": "jacobemerick",
            "email": "jpemeric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "avalanche-development/swagger-router-middleware": "^0.3",
        "zendframework/zend-diactoros": "^1.3",
        "avalanche-development/peel": "^0.2",
        "avalanche-development/crash-pad": "^0.2"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "AvalancheDevelopment\\Talus\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/function-overrides.php"
        ],
        "psr-4": {
            "AvalancheDevelopment\\Talus\\": "tests/"
        }
    }
}
